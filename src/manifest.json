{
  "manifest_version": 2,
  "name": "close-bullet-screen",
  "version": "1.0.0",
  "description": "自动关闭网页中默认开启的弹幕",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.huya.com/*", "https://www.bilibili.com/*", "https://www.douyu.com/*"],
      "js": ["js/jquery.min.js", "js/page.js"],
      "run_at": "document_end"
    }
  ]
}
